#include <iostream>
using namespace std;


void reverse(int a[],int size ){
    for (int i=size-1;i>=0;i--){
        cout<<a[i]<<" ";
        
    }
    
    
}
int main() {
    
    int size;
    cin>>size;
    int a[size];
    for (int i=0;i<size;i++){
        cin>>a[i] ;
    
    }
    
    reverse(a,size);
    return 0;
}


